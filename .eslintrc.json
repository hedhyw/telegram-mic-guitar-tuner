{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ]
  }
}